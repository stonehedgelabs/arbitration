/**
 * Cloudflare Worker proxy for dev.mysite.com â†’ ngrok
 * Docs: https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "arb-dev-proxy",
	"main": "src/index.ts",
	"compatibility_date": "2025-10-11",

	"routes": [
		{
			"pattern": "dev.arbi.gg/*",
			"zone_name": "arbi.gg"
		}
	],

	"observability": {
		"enabled": true
	}
}
